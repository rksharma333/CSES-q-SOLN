#include<bits/stdc++.h>
using namespace std;
#define ll long long 
int  mod = 1e9+7;
int main()
{
    ll n;
    cin>>n;
    ll ans=0;
    for(ll i=5;i<=n;i=i+5)
    {
        ll num=i;
        while(num>0)
        {
            if(num%5!=0) break;
            if(num%5==0) ans++;
            num=num/5;
        }
    
    }
    
    cout<<ans<<endl;
}
