#include<bits/stdc++.h>
using namespace std;
int main()
{
   
    int n;
    cin>>n;
    vector<int> v(n-1);
    for(int i=0;i<n;i++) cin>>v[i];
    sort(v.begin(),v.end());
    int cnt=0;
    int j=0;
    for(int i=0;i<n;i++)
    {
        j++;
        if(j==v[i]) continue;
        
        break;
    }
    cout<<j<<endl;
}
